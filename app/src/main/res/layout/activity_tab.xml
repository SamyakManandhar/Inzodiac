<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:keepScreenOn="true">


        <FrameLayout
            android:id="@+id/frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/linermain"
            android:background="@color/white"

            />

        <LinearLayout
            android:id="@+id/linermain"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_alignParentBottom="true"
            android:background="@color/white"
            android:elevation="10dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:weightSum="5">

            <RelativeLayout
                android:id="@+id/one"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/white"
                android:orientation="vertical">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgOne"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitCenter"
                    app:srcCompat="@drawable/ic_inactive_menu" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/two"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgTwo"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitCenter"
                    app:srcCompat="@drawable/ic_inactive_like" />


            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/three"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/white">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgThree"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitCenter"
                    app:srcCompat="@drawable/ic_inactive_messages" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/four"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:background="@color/white">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgFour"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitCenter"
                    app:srcCompat="@drawable/ic_inactive_profile" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/five"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/white">


                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgFive"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitCenter"
                    app:srcCompat="@drawable/ic_inactive_zodiac" />


            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
</layout>
